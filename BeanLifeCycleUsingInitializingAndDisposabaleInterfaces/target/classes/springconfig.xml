<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="employee" class="com.nsksoft.domain.Employee"
		init-method="afterPropertiesSet" destroy-method="destroy">
		<property name="empName" value="Mr.Xyz"></property>
		<property name="presentAddress" value="Bangalore"></property>
	</bean>

</beans>
